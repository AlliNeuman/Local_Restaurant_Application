<h1>Browse Restaurants</h1>

<br>
<div class="form-all index-page">
<% @created_restaurants.each do |created_restaurant| %>
<div>
  <a href="/restaurants/<%=created_restaurant.id%>"><%=created_restaurant.name%></a>
  <p><%=created_restaurant.category%>
    <br>
  <%=created_restaurant.neighborhood%></p>
</div>
<% end %>
</div>
